<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="MAUISampleDemo.View.Email_SMS_PhoneDialerDemo"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:maps="clr-namespace:Microsoft.Maui.Controls.Maps;assembly=Microsoft.Maui.Controls.Maps"
    Title="Email_SMS_PhoneDialerDemo">
    <VerticalStackLayout Margin="0,20,0,0" Spacing="10">
        <Button
            x:Name="btnPhoneDial"
            Margin="20,20,20,0"
            Clicked="btnPhoneDial_Clicked"
            Text="Phone Dial" />

        <Button
            x:Name="btnsms"
            Margin="20,20,20,0"
            Clicked="btnsms_Clicked"
            Text="SMS" />

        <Button
            x:Name="btnEmail"
            Margin="20,20,20,0"
            Clicked="btnEmail_Clicked"
            Text="Email" />

        <Ellipse
            HeightRequest="150"
            Stroke="Red"
            StrokeThickness="4"
            WidthRequest="150" />

    </VerticalStackLayout>
</ContentPage>